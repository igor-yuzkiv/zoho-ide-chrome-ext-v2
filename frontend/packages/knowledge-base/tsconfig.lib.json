{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "outDir": "../../dist/knowledge-base",
        "types": ["vite/client"],
        "rootDir": "src",
        "jsx": "preserve",
        "jsxImportSource": "vue",
        "module": "esnext",
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "tsBuildInfoFile": "dist/tsconfig.lib.tsbuildinfo"
    },
    "include": ["src/**/*.js", "src/**/*.jsx", "src/**/*.ts", "src/**/*.tsx", "src/**/*.vue"],
    "exclude": ["eslint.config.js", "eslint.config.cjs", "eslint.config.mjs"],
    "references": [
        {
            "path": "../ui-kit/tsconfig.lib.json"
        },
        {
            "path": "../tags/tsconfig.lib.json"
        },
        {
            "path": "../attachments/tsconfig.lib.json"
        },
        {
            "path": "../shared/tsconfig.lib.json"
        }
    ]
}
